<h1>Posts</h1>
<% if @current_user %>
  <%= link_to 'New Post', new_post_path %> |
  <%= link_to 'Logout', session_path(:current), method: :delete %>
<% else %>
  <%= link_to 'Login', new_session_path %> |
  <%= link_to 'Sign Up', new_user_path %>
<% end %>
<ul>
  <% @posts.each do |post| %>
    <li><%= link_to post.title, post %> by <%= post.user.username %></li>
  <% end %>
</ul>
